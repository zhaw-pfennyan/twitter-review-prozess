    </bpmn:userTask>
    <bpmn:userTask id="TweetAnfrageUeberarbeiten" name="Tweet-Anfrage überarbeiten" camunda:assignee="${anfrageStellenderBenutzer}" camunda:candidateGroups="kommunikationsabteilung">
